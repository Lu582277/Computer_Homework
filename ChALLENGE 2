print("Please enter the list of NUMBERS you wish to find the largest number from: ")
#user will edit list here
nums = [5,7,435,4]


def find_max(nums):
	maximum = nums[0]
	for i in range(1, len(nums)):
		if nums[i] > maximum:
			maximum = nums[i]
	print("The largest number is: ", maximum)
	return(maximum)

find_max(nums)
